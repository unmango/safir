<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Product>Safir.Agent.Client.Abstractions</Product>
    <RootNamespace>Safir.Agent.Client</RootNamespace>
    <Description>Abstractions for a client that can make requests to a Safir agent.</Description>
    <TargetFrameworks>netstandard2.1;net5.0;net6.0</TargetFrameworks>
    <IsPackable>true</IsPackable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Grpc.Core" Version="2.39.1" />
    <PackageReference Include="JetBrains.Annotations" Version="2021.2.0" />
    <PackageReference Include="MinVer" Version="2.5.0" PrivateAssets="All" />
    <PackageReference Include="Safir.Protos" Version="0.7.12" />
  </ItemGroup>

</Project>
