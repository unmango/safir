<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Product>Safir.Serialization.Protobuf</Product>
    <Description>Protobuf implementation of Safir.Common.ISerializer</Description>
    <TargetFramework>netstandard2.0</TargetFramework>
    <IsPackable>true</IsPackable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Google.Protobuf" Version="3.21.7" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="6.0.0" />
    <PackageReference Include="MinVer" Version="4.2.0" PrivateAssets="all" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="$(CommonSrc)\Safir.Common.Abstractions\Safir.Common.Abstractions.csproj" />
  </ItemGroup>

</Project>
