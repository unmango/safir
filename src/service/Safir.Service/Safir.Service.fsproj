<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <EnableSdkContainerSupport>true</EnableSdkContainerSupport>
  </PropertyGroup>

  <ItemGroup>
    <None Include="../protos/safir/**/*.proto" Link="protos/%(RecursiveDir)/$(FileName).proto" />
    <Compile Include="Generated/Files.fs" />
    <Compile Include="Generated/Host.fs" />
    <Compile Include="Generated/Media.fs" />
    <Compile Include="Generated/MediaDir.fs" />
    <Compile Include="Config.fs" />
    <Compile Include="Files.fs" />
    <Compile Include="MediaDir.fs" />
    <None Include="Library.fs" />
    <Compile Include="Services\StartupScanner.fs" />
    <None Include="Services\DataWatcher.fs" />
    <Compile Include="Services\FilesService.fs" />
    <Compile Include="Program.fs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Equinox.EventStoreDb" />
    <PackageReference Include="EventStore.Client.Grpc.Streams" />
    <PackageReference Include="FsCodec.SystemTextJson" />
    <PackageReference Include="FSharp.Control.AsyncRx" />
    <PackageReference Include="FSharp.Control.AsyncSeq" />
    <PackageReference Include="FSharp.UMX" />
    <PackageReference Include="Grpc.AspNetCore.Server" />
    <PackageReference Include="Grpc.AspNetCore.Server.Reflection" />
    <PackageReference Include="Microsoft.Extensions.Configuration" />
    <PackageReference Include="Propulsion.EventStoreDb" />
  </ItemGroup>

</Project>
